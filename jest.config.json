{
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/mocks/chromeMock.js",
    "\\.(css|less|scss|sass)$": "identity-obj-proxy"
  },
  "setupFiles": ["jest-canvas-mock", "<rootDir>/jest/setup.js"],
  "collectCoverage": true,
  "collectCoverageFrom": ["**/*.{js, jsx}"],
  "coveragePathIgnorePatterns": [
    "<rootDir>/build/",
    "<rootDir>/node_modules/",
    "<rootDir>/coverage/",
    "<rootDir>/src/images/",
    "<rootDir>/src/SVG/",
    "<rootDir>/src/setupTests.js",
    "<rootDir>/public/firebaseInit.js",
    "<rootDir>/src/serviceWorker.js",
    "<rootDir>/babel.config.js",
    "<rootDir>/out/",
    "<rootDir>/public/LandingPage/js/materialize.js",
    "<rootDir>/public/LandingPage/js/materialize.min.js"
  ],
  "coverageReporters": ["json", "text", "text-summary", "html", "lcov"],
  "coverageThreshold": {
    "global": {
      "lines": 20,
      "statements": 20
    }
  },
  "verbose": false
}
