{
  "name": "react merged code",
  "description": "",
  "version": "1",
  "manifest_version": 2,
  "key" : "",
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/robots.txt"],
      "js": ["github-oauth.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": ["identity", 
    "identity.email", 
    "tabs", 
    "storage",
    "https://www.github.com/",
    "https://api.github.com"

  ],
  "web_accessible_resources": ["index.html", "/static/*", "oauth2/oauth2.html"],
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com 'unsafe-eval'; object-src 'self'"
}
